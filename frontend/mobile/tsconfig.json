{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@services/*": ["../services/*"],
      "@utils/*": ["../utils/*"],
      "@assets/*": ["../assets/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@types/*": ["./src/types/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@storage/*": ["./src/storage/*"],
    }
  }
}
